<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.findai.xkk.ai_interviewer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:name=".MyApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".WelcomeIndexActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".JobCenterActivity"></activity>
        <activity android:name=".WelcomeInterviewActivity"></activity>
        <activity android:name=".RegisterActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SubmitReportActivity"></activity>
        <activity android:name=".ViewReportWeb"></activity>
        <activity android:name=".InterviewMainActivity"></activity>
        <activity android:name=".Resume_Warehouse_Activity"></activity>
        <activity android:name=".JobinfoActivity"></activity>
        <activity android:name=".WriteResumeActivity"></activity>
    </application>

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE"/>
    <!-- 添加NETWORK 权限 -->
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

</manifest>